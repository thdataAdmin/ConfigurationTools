<Window x:Class="ProductionPlanning.ConfigurationTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProductionPlanning.ConfigurationTool"
        mc:Ignorable="d"
        Title="Planeus licence manager" Height="600" Width="900">
    <Grid>
        <Button Command="{Binding ShowGenerateLicenceDialogCommand}" Content="Lizenz generieren" HorizontalAlignment="Left" Margin="10,29,0,0" VerticalAlignment="Top" Width="109" RenderTransformOrigin="-0.667,-0.9"/>
        <DataGrid Margin="10,92,10,10"/>
        <DockPanel>
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="Datei">
                    <MenuItem Header="_Open"/>
                </MenuItem>
            </Menu>
            <StackPanel></StackPanel>
        </DockPanel>
    </Grid>
</Window>
